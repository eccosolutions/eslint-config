{
  "name": "@softwareventures/eslint-config",
  "version": "0.0.0-development",
  "description": "Standard eslint configuration for Software Ventures Limited",
  "keywords": [
    "TypeScript",
    "eslint",
    "eslint-config"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/eslint-config",
  "bugs": "https://github.com/softwareventures/eslint-config",
  "repository": "github:softwareventures/eslint-config",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "pre-commit": "precise-commits",
    "prepare": "husky install && tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^7.0.0 || ^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsdoc": "^37.0.3 || ^38.0.0 || ^39.0.0 || ^40.0.0 || ^41.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-sonarjs": "^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^0.14.0 || ^0.15.0 || ^0.16.0 || ^0.17.0 || ^0.18.0 || ^0.19.0 || ^0.20.0 || ^0.21.0 || ^0.22.0 || ^0.23.0",
    "typescript": "^3.8.3 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@softwareventures/precise-commits": "3.0.21",
    "@softwareventures/prettier-config": "4.0.0",
    "@softwareventures/semantic-release-config": "4.0.0",
    "@softwareventures/tsconfig": "7.0.0",
    "@types/eslint": "8.44.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "ava": "5.3.1",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "41.1.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-sonarjs": "0.23.0",
    "husky": "8.0.3",
    "prettier": "3.1.0",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": "."
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}

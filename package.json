{
  "name": "@softwareventures/eslint-config",
  "version": "0.0.0-development",
  "description": "Standard eslint configuration for Software Ventures Limited",
  "keywords": [
    "eslint",
    "eslint-config",
    "TypeScript"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/eslint-config",
  "bugs": "https://github.com/softwareventures/eslint-config",
  "repository": "github:softwareventures/eslint-config",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "prettier --check . && eslint . --ext .ts,.tsx && ava",
    "fix": "eslint . --ext .ts,.tsx --fix && prettier --write ."
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.31.0 || ^3.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.31.0 || ^3.0.0",
    "@typescript-eslint/parser": "^2.31.0 || ^3.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.0.1 || ^26.0.0 || ^27.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "tslib": "^1.11.2 || ^2.0.0",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@softwareventures/prettier-config": "1.0.2",
    "@softwareventures/semantic-release-config": "1.0.2",
    "@softwareventures/tsconfig": "2.2.0",
    "@types/eslint": "7.2.0",
    "ava": "3.8.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.2.0",
    "husky": "4.2.5",
    "precise-commits": "1.0.2",
    "prettier": "2.0.5",
    "semantic-release": "17.0.8",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "eslint": "^7.0.0",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": "."
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  }
}
